float4x4 matViewProjection;
float4x4 matWorldView;

struct VS_OUTPUT 
{
   float4 Pos:     POSITION;
   float3 Normal:  TEXCOORD0;
   float3 WorldPos:TEXCOORD1;
};

VS_OUTPUT vs_main( float4 inPos: POSITION, float3 inNormal: NORMAL )
